
<% layout('layout/default') %>
<div class="col-sm-8 col-sm-offset-2 mt-100">
<div class="panel panel-default">
    <div class="panel-heading" style="background-color: #E53844!important;">
        <h3 class="text-center text-white">DoctorDial</h3>
    </div>
<div class="panel-body">
    <h3 class="text-danger"><i class="fa fa-times-circle-o"></i> Error!</h3>
    <% if(typeof error !="undefined"){ %>
    <p class="text-danger"> <%- error %> </p>
    <% } %>

</div>
</div>
</div>